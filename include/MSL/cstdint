#ifndef MSL_CPP_CSTDINT_H
#define MSL_CPP_CSTDINT_H
#include <stdint.h>
#ifdef __cplusplus

namespace std {
using ::int8_t;
using ::int16_t;
using ::int32_t;
using ::int64_t;

using ::uint8_t;
using ::uint16_t;
using ::uint32_t;
using ::uint64_t;

using ::intmax_t;
using ::uintmax_t;

using ::intptr_t;
using ::uintptr_t;
} // namespace std


#endif
#endif
